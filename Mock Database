CREATE TABLE STUDENT(
    FNAME            VARCHAR2(20),
    LNAME            VARCHAR2(20),
    Status           VARCHAR2(20),
    City             VARCHAR2(20),
    Degree           VARCHAR2(20),
    EMAIL            VARCHAR2(50)
                     PRIMARY KEY,
    Experience       VARCHAR2(1000),
    studentSkills    VARCHAR(100)
);

CREATE TABLE JOBS (
    JobTitle        VARCHAR2(35),
    City            VARCHAR2(20),
    Field           VARCHAR2(20),
    jobs_Skills     VARCHAR2(100)
);

CREATE TABLE SKILLS (
    Skills           VARCHAR2(100)
);

